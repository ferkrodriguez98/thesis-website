#: env/lib/python3.11/site-packages/django/contrib/postgres/apps.py:54
msgid "PostgreSQL extensions"
msgstr "Extensions de PostgreSQL"

#: env/lib/python3.11/site-packages/django/contrib/postgres/fields/array.py:21
#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/array.py:17
#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/array.py:185
#, python-format
msgid "Item %(nth)s in the array did not validate:"
msgstr "L'element %(nth)s de la matriu no s'ha pogut validar:"

#: env/lib/python3.11/site-packages/django/contrib/postgres/fields/array.py:22
msgid "Nested arrays must have the same length."
msgstr "Les matrius niades han de tenir la mateixa longitud."

#: env/lib/python3.11/site-packages/django/contrib/postgres/fields/hstore.py:15
msgid "Map of strings to strings/nulls"
msgstr "Mapa de cadenes a cadenes/nuls"

#: env/lib/python3.11/site-packages/django/contrib/postgres/fields/hstore.py:17
#, python-format
msgid "The value of “%(key)s” is not a string or null."
msgstr "El valor de \"%(key)s\" no és ni una cadena ni un nul."

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/hstore.py:17
msgid "Could not load JSON data."
msgstr "No es poden carregar les dades JSON"

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/hstore.py:18
msgid "Input must be a JSON dictionary."
msgstr "L'entrada ha de ser un diccionari JSON"

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:42
msgid "Enter two valid values."
msgstr "Introdueixi dos valors vàlids."

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:44
msgid "The start of the range must not exceed the end of the range."
msgstr "L'inici del rang no pot excedir el seu final."

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:99
msgid "Enter two whole numbers."
msgstr "Introduïu dos números enters positius."

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:105
msgid "Enter two numbers."
msgstr "Introduïu dos números."

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:111
msgid "Enter two valid date/times."
msgstr "Introduïu dues data/hora vàlides."

#: env/lib/python3.11/site-packages/django/contrib/postgres/forms/ranges.py:117
msgid "Enter two valid dates."
msgstr "Introduïu dos dates vàlides."

#: env/lib/python3.11/site-packages/django/contrib/postgres/validators.py:15
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no more than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no more than "
"%(limit_value)d."
msgstr[0] ""
"La llista conté %(show_value)d element, no n'hauria de tenir més de "
"%(limit_value)d."
msgstr[1] ""
"La llista conté %(show_value)d elements, no n'hauria de tenir més de "
"%(limit_value)d."

#: env/lib/python3.11/site-packages/django/contrib/postgres/validators.py:25
#, python-format
msgid ""
"List contains %(show_value)d item, it should contain no fewer than "
"%(limit_value)d."
msgid_plural ""
"List contains %(show_value)d items, it should contain no fewer than "
"%(limit_value)d."
msgstr[0] ""
"La llista conté %(show_value)d element, no n'hauria de contenir menys de "
"%(limit_value)d."
msgstr[1] ""
"La llista conté %(show_value)d elements, no n'hauria de contenir menys de "
"%(limit_value)d."

#: env/lib/python3.11/site-packages/django/contrib/postgres/validators.py:38
#, python-format
msgid "Some keys were missing: %(keys)s"
msgstr "Algunes claus no hi són: %(keys)s"

#: env/lib/python3.11/site-packages/django/contrib/postgres/validators.py:39
#, python-format
msgid "Some unknown keys were provided: %(keys)s"
msgstr "S'han facilitat claus desconegudes: %(keys)s"

#: env/lib/python3.11/site-packages/django/contrib/postgres/validators.py:81
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely greater than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the upper bound of the range is not greater than %(limit_value)s."
msgstr ""
"Asseguri's que aquest rang és completament major o igual a %(limit_value)s."

#: env/lib/python3.11/site-packages/django/contrib/postgres/validators.py:90
#, fuzzy, python-format
#| msgid ""
#| "Ensure that this range is completely less than or equal to "
#| "%(limit_value)s."
msgid ""
"Ensure that the lower bound of the range is not less than %(limit_value)s."
msgstr ""
"Asseguri's que aquest rang és completament menor o igual a %(limit_value)s."
