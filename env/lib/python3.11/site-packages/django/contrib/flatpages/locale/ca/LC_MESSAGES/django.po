#: env/lib/python3.11/site-packages/django/contrib/flatpages/admin.py:13
msgid "Advanced options"
msgstr "Opcions avançades"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/apps.py:8
msgid "Flat Pages"
msgstr "Pàgines Estàtiques"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:11
#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:9
msgid "URL"
msgstr "URL"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:15
msgid ""
"Example: “/about/contact/”. Make sure to have leading and trailing slashes."
msgstr ""
"Exemple: '/about/contact/'. Assegureu-vos de posar les barres al principi i "
"al final."

#: env/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:20
msgid ""
"This value must contain only letters, numbers, dots, underscores, dashes, "
"slashes or tildes."
msgstr ""
"Aquest valor sols pot contenir lletres, nombres, punts, subratllats, guions, "
"barres o accents."

#: env/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:34
msgid "Example: “/about/contact”. Make sure to have a leading slash."
msgstr ""
"Exemple: '/about/contact/'. Assegureu-vos de posar la barra al principi."

#: env/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:47
msgid "URL is missing a leading slash."
msgstr "La URL no comença amb \"/\"."

#: env/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:52
msgid "URL is missing a trailing slash."
msgstr "La URL no acaba amb \"/\"."

#: env/lib/python3.11/site-packages/django/contrib/flatpages/forms.py:69
#, python-format
msgid "Flatpage with url %(url)s already exists for site %(site)s"
msgstr "Ja hi ha una pàgina estàtica amb la URL %(url)s per al lloc %(site)s"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:10
msgid "title"
msgstr "títol"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:11
msgid "content"
msgstr "contingut"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:12
msgid "enable comments"
msgstr "habilitar comentaris"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:14
msgid "template name"
msgstr "nom de la plantilla"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:18
msgid ""
"Example: “flatpages/contact_page.html”. If this isn’t provided, the system "
"will use “flatpages/default.html”."
msgstr ""
"Exemple: 'flatpages/contact_page.html'. Si no es proporciona, el sistema "
"utilitzarà 'flatpages/default.html'."

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:23
msgid "registration required"
msgstr "cal estar registrat"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:25
msgid "If this is checked, only logged-in users will be able to view the page."
msgstr "Si està marcat, només els usuaris registrats podran veure la pàgina."

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:29
msgid "sites"
msgstr "llocs"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:33
msgid "flat page"
msgstr "pàgina estàtica"

#: env/lib/python3.11/site-packages/django/contrib/flatpages/models.py:34
msgid "flat pages"
msgstr "pàgines estàtiques"
